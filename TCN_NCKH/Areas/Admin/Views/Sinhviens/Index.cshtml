@model IEnumerable<TCN_NCKH.Models.DBModel.Sinhvien>
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Msv)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Lophoc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SinhvienNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Msv)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Lophoc.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SinhvienNavigation.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Sinhvienid">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Sinhvienid">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Sinhvienid">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
